import castToType from '../dataTypes/castToType';
import atomize from '../dataTypes/atomize';
function fnTrace (dynamicContext, arg, label) {
	const argumentAsStrings = arg.getAllValues().map(value => castToType(atomize(value, dynamicContext), 'xs:string'));
	console.debug.apply(console, label ? [argumentAsStrings, label.first().value] : [argumentAsStrings]);
	return arg;
}

export default {
	declarations: [
		{
			name: 'trace',
			argumentTypes: ['item()*'],
			returnType: 'item()*',
			callFunction: fnTrace
		}

	]
};
