import adaptJavaScriptValueToXPathValue from 'fontoxml-selectors/selectors/dataTypes/adaptJavaScriptValueToXPathValue';

describe('adaptJavaScriptValueToXPathValue', () => {
	it(
		'turns numbers into integers',
		() => chai.assert.equals(adaptJavaScriptValueToXPathValue(1, 'xs:integer').instanceOfType('xs:integer'))
});
