import slimdom from 'slimdom';

import blueprint from 'fontoxml-blueprints/readOnlyBlueprint';
import evaluateXPathToString from 'fontoxml-selectors/evaluateXPathToString';
import evaluateXPathToMap from 'fontoxml-selectors/evaluateXPathToMap';

let documentNode;
beforeEach(() => {
	documentNode = slimdom.createDocument();
});

describe('functions over json', () => {
	it(
		'can parse json',
		() => chai.assert.deepEqual(evaluateXPathToMap('parse-json("{"""a""": 1}")')));
});
