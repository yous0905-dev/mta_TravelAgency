/**
* @fileoverview
* @externs
*/

/**
 * @interface
 */
class INodesFactory {

/**
 * @nosideeffects
 * @export
 * @param   {!Node}  _node
 * @return  {?Node}
 */
getParentNode (_node) {
}

/**
 * @nosideeffects
 * @export
 * @param   {!Node}  _node
 * @return  {?Node}
 */
getFirstChild (_node) {
}

/**
 * @nosideeffects
 * @export
 * @param   {!Node}  _node
 * @return  {?Node}
 */
getLastChild (_node) {
}

/**
 * @nosideeffects
 * @export
 * @param   {!Node}  _node
 * @return  {?Node}
 */
getNextSibling (_node) {
}

/**
 * @nosideeffects
 * @export
 * @param   {!Node}  _node
 * @return  {?Node}
 */
getPreviousSibling (_node) {
}

/**
 * @nosideeffects
 * @export
 * @param   {!Node}  _node
 * @return  {!Array<!Node>}
 */
getChildNodes (_node) {
}

/**
 * @nosideeffects
 * @export
 * @param   {!Element}    _node
 * @param   {string}   _attributeName
 * @return  {?string}
 */
getAttribute (_node, _attributeName) {
}

/**
 * @nosideeffects
 * @export
 * @param   {!Node}    _node
 * @return  {!Array<{name:!string,value:!string}>}
 */
getAllAttributes (_node) {
}

/**
 * @nosideeffects
 * @export
 * @param   {!Node|*}    _node
 * @return  {string}
 */
getData (_node) {
}

/**
 * @nosideeffects
 * @export
 * @param   {!Node}                                    _node
 * @param   {function(Node, INodesFactory):!Array<Node>}  _callback
 * @return  {!Array<Node>}
 */
getRelatedNodes (_node, _callback) {
}
